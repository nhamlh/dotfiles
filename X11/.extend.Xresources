*background:            #272827
*foreground:            #657b83
*fading:                15
*fadeColor:             black
*cursorColor:           #16A085
*pointerColorBackground:#2B2C2B
*pointerColorForeground:#16A085

URxvt.internalBorder: 6

! special
*.foreground:   #bfbfbf
*.background:   #162025
*.cursorColor:  #bfbfbf

! black
*.color0:       #0f1619
*.color8:       #5d6160

! red
*.color1:       #393843
*.color9:       #662b37

! green
*.color2:       #75404b
*.color10:      #39402e

! yellow
*.color3:       #39402e
*.color11:      #393843

! blue
*.color4:       #56694d
*.color12:      #75404b

! magenta
*.color5:       #4c584a
*.color13:      #662b37

! cyan
*.color6:       #662b37
*.color14:      #4c584a

! white
*.color7:       #bfbfbf
*.color15:      #bfbfbf


Xcursor.theme: Breeze
Xcursor.size:  0

XTerm*background: #6aa7a5
XTerm*foreground: #6aa7a5
XTerm*reverseVideo: on
!XTerm*faceName: Terminus:size=11:antialias=true
!XTerm*faceName: InconsolataRegular:size=11:antialias=true
XTerm*faceName: HackRegular:size=11:antialias=true
XTerm*selectToClipboard: true

!URxvt.font: xft:Hack:size=10
URxvt.font: xft:monofur:size=12
URxvt.geometry: 80x20
URxvt.depth: 32
URxvt.background:	[100]#162025
URxvt*scrollBar:                      false
URxvt*mouseWheelScrollPage:           false
URxvt*cursorBlink:                    true
!URxvt*.background:   		      #162025
URxvt*.foreground: 		      #bfbfbf
URxvt*cursorColor:		      #bfbfbf
URxvt*saveLines:                      5000
! ROFI SETUP HERE !

! Enable the extended coloring options
rofi.color-enabled: true
!                  bg       border   separator
rofi.color-window: #162025, #662b37, #662b37
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #162025, #bfbfbf, #162025, #162025, #662b37
rofi.color-active: #162025, #bfbfbf, #162025, #162025, #662b37
rofi.color-urgent: #162025, #bfbfbf, #162025, #162025, #662b37

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
!rofi.font: Hack Regular 12
rofi.font: monofur 13
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true



